{
    "name": "upch/yii2-theme",
    "description": "Template institucional",
    "type": "upch-yii2-extension",
    "require": {
        "yiisoft/yii2": "^2.0"
    },
    "authors": [
        {
            "name": "JÃ³se Nolberto Vilchez Moreno",
            "email": "jnolbertovm@gmail.com"
        }
    ],
    "config": {
        "process-timeout": 1800,
        "vendor-dir": "dist/vendor",
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "dist/vendor/npm",
                "bower-asset-library": "dist/vendor/bower"
            }
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "post-update-cmd": [
            "bower install"
        ],
        "post-install-cmd": [
            "bower install --production"
        ]
    },
    "autoload": {
        "psr-4": {
            "upch\\theme\\": ""
        }
    }
}
